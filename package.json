{
    "name": "node_starter",
    "version": "1.0.0",
    "description": "A Node Starter",
    "main": "index.js",
    "scripts": {
        "devstart": "babel-node src/index.js",
        "start": "node ./build/index.js",
        "build": "npm run clean && npm run babel-build",
        "babel-build": "babel -d ./build ./src -s",
        "clean": "mkdir build"
    },
    "author": "Regie Osano",
    "license": "MIT",
    "dependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "express": "^4.17.3"
    }
}